{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/static/*", "/images/*", "/*.{css,scss,js,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/_next/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "trailingSlash": "auto"
}
